#Gibbs Free Energy#
##### All Variables #####
model <- lm(G ~ P+AP+PWC+APWC+BW)


#Plot#
layout(matrix(c(1,2,3,4),2,2))
plot(model)
#Statistical analysis#
summary(model)


##### Most Significant #####
model <- lm(G ~ AP+PWC+APWC+BW)


#Plot#
layout(matrix(c(1,2,3,4),2,2))
plot(model)
#Statistical analysis#
summary(model)

##### Significant #####
model <- lm(G ~ AP+PWC+APWC)


#Plot#
layout(matrix(c(1,2,3,4),2,2))
plot(model)
#Statistical analysis#
summary(model)

##### Add Polar Counts Significance Drops #####
model <- lm(G ~ P+AP+PWC+APWC)


#Plot#
layout(matrix(c(1,2,3,4),2,2))
plot(model)
#Statistical analysis#
summary(model)


##### Binding water significance #####
model <- lm(G ~ AP+APWC+BW)

#Plot#
layout(matrix(c(1,2,3,4),2,2))
plot(model)

#Statistical analysis#
summary(model)


########## ENTHALPY ##########
##### All Variables #####
model <- lm(H ~ P+AP+PWC+APWC+BW)


#Plot#
layout(matrix(c(1,2,3,4),2,2))
plot(model)
#Statistical analysis#
summary(model)

########## ENTROPY ##########
